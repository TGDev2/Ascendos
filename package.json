{
  "name": "ascendos",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "type-check": "turbo type-check",
    "clean": "turbo clean && rm -rf node_modules",
    "smoke-test": "tsx scripts/smoke-test.ts",
    "smoke-test:prod": "DEPLOYMENT_URL=https://your-production-url.vercel.app tsx scripts/smoke-test.ts"
  },
  "devDependencies": {
    "@turbo/gen": "^2.3.3",
    "@types/node": "^22.10.5",
    "prettier": "^3.4.2",
    "tsx": "^4.19.2",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  }
}
